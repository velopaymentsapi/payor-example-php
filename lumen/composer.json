{
    "name": "velopaymentsapi/payor-example-php",
    "description": "Velo Payor Example.",
    "keywords": [
        "velo",
        "velo-payments",
        "lumen",
        "laravel",
        "velo-php"
    ],
    "license": "proprietary",
    "type": "project",
    "require": {
        "php": "^7.2",
        "firebase/php-jwt": "^5.0",
        "laravel/lumen-framework": "6.3.*",
        "ramsey/uuid": "^3.8",
        "velopaymentsapi/velo-php": "2.11.73",
        "vlucas/phpdotenv": "^3.3",
        "ext-zip": "*"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.4",
        "phpunit/phpunit": "^7.0",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
